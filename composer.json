                  "rfc5545",
                  "rfc6321",
                  "management",
                  "icalendar",
                  "calendar",
                  "vCalendar",
                  "vevent",
                  "vtodo",
                  "vjournal",
                  "vfreebusy",
                  "valarm",
                  "vtimezone",
                  "standard",
                  "daylight",
                  "file" ],
    "time" : "2019-10-31",
    "license": "GPL-3.0-or-later",
    "homepage": "https://icalendar.org/php-library.html",
    "authors": [
        {
            "name": "Bruce Wells",
            "email": "brucekwells@gmail.com"
        }
		],
    "autoload": {
        "psr-0": { "ICalendarOrg": ["src/"] }
    },
    "require": {
        "php": ">=7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4"
    }
